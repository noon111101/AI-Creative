{
  "rewrites": [
    {
      "source": "/ai-sandbox-videofx/:match*",
      "destination": "https://storage.googleapis.com/ai-sandbox-videofx/:match*"
    },
    {
      "source": "/v1:uploadUserImage",
      "destination": "/api/proxy?match=:uploadUserImage"
    },
    {
      "source": "/v1/:match*",
      "destination": "/api/proxy"
    }
  ]
}